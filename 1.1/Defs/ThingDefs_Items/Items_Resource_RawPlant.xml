<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="RoughPlantBase">
    <defName>MateLeaves</defName>
    <label>yerba mate leaves</label>
    <description>Raw cut leaves of a yerba mate tree. Can be made into a mate at a crafting spot.</description>
    <graphicData>
      <texPath>Things/Item/Resource/PlantFoodRaw/SmokeleafLeaves</texPath>
    </graphicData>
    <statBases>
      <MarketValue>1.6</MarketValue>
      <Flammability>1.3</Flammability>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>30</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <thingCategories>
      <li>PlantMatter</li>
    </thingCategories>
    <ingestible>
      <preferability>DesperateOnly</preferability>
    </ingestible>
  </ThingDef>
  
</Defs>
